    		//forward
    		//if( ! execRobotMove("findTheDoorRight","forward",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "sonarDetect,local_alarm,local_takeControl" , "deadEndRight,handleAlarm,handleTakeControl") ) break;
