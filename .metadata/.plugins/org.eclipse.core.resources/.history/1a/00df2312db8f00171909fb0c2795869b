    while(true){
    	curPlanInExec =  "reachFinalPosition";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN REACH FINAL POSITION------------------\"";
    		println( temporaryStr );  
    		parg = "actorOp(saveStart)";
    		aar = solveGoalReactive(parg,3600000,"","");
    		//println(getName() + " plan " + curPlanInExec  +  " interrupted=" + aar.getInterrupted() + " action goon="+aar.getGoon());
    		if( aar.getInterrupted() ){
    			curPlanInExec   = "reachFinalPosition";
    			if( aar.getTimeRemained() <= 0 ) addRule("tout(actorOp,"+getName()+")");
    			if( ! aar.getGoon() ) break;
    		} 			
    		//QActorUtils.solveGoal(parg,pengine );
    		temporaryStr = "goingForward";
    		addRule( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?forwardSpeed(FS)" )) != null ){
    		//forward
    		//if( ! execRobotMove("reachFinalPosition","forward",Integer.parseInt(guardVars.get("FS")),0,60000, "local_goal,local_alarm,local_takeControl" , "handleGoal,handleAlarm,handleTakeControl") ) break;
    		    aar = execRobotMove("reachFinalPosition","forward",Integer.parseInt(guardVars.get("FS")),0,60000, "local_goal,local_alarm,local_takeControl" , "handleGoal,handleAlarm,handleTakeControl");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "reachFinalPosition";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " ??goingForward" )) != null ){
    		if( ! planUtils.switchToPlan("saveForwardPath").getGoon() ) break;
    		}
    		if( planUtils.repeatPlan(nPlanIter,0).getGoon() ) continue;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=reachFinalPosition WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean saveForwardPath() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "saveForwardPath";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "saveForwardPath";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN SAVE PATH------------------\"";
    		println( temporaryStr );  
    		parg = "actorOp(getDurationTime)";
    		aar = solveGoalReactive(parg,3600000,"","");
    		//println(getName() + " plan " + curPlanInExec  +  " interrupted=" + aar.getInterrupted() + " action goon="+aar.getGoon());
    		if( aar.getInterrupted() ){
    			curPlanInExec   = "saveForwardPath";
    			if( aar.getTimeRemained() <= 0 ) addRule("tout(actorOp,"+getName()+")");
    			if( ! aar.getGoon() ) break;
    		} 			
    		//QActorUtils.solveGoal(parg,pengine );
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?saveMove(fwd)" )) != null ){
    		//we should restore nPlanIter and curPlanInExec of the 'interrupted' plan ???
    		returnValue = continueWork;
    		}
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=saveForwardPath WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean saveRemainingTime() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "saveRemainingTime";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "saveRemainingTime";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN SAVE REMAINING TIME------------------\"";
    		println( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?deleteLastMove(MOVE)" )) != null ){
    		temporaryStr = "lastMoveDeleted(MOVE)";
    		temporaryStr = QActorUtils.substituteVars(guardVars,temporaryStr);
    		println( temporaryStr );  
    		}
    		parg = "actorOp(getRemainingTime)";
    		aar = solveGoalReactive(parg,3600000,"","");
    		//println(getName() + " plan " + curPlanInExec  +  " interrupted=" + aar.getInterrupted() + " action goon="+aar.getGoon());
    		if( aar.getInterrupted() ){
    			curPlanInExec   = "saveRemainingTime";
    			if( aar.getTimeRemained() <= 0 ) addRule("tout(actorOp,"+getName()+")");
    			if( ! aar.getGoon() ) break;
    		} 			
    		//QActorUtils.solveGoal(parg,pengine );
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?saveMove(fwd)" )) != null ){
    		//we should restore nPlanIter and curPlanInExec of the 'interrupted' plan ???
    		returnValue = continueWork;
    		}
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=saveRemainingTime WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean standby() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "standby";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "standby";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN STANDBY------------------\"";
    		println( temporaryStr );  
    		//senseEvent
    		aar = planUtils.senseEvents( 10000,"local_start","handleStart",
    		"" , "",ActionExecMode.synch );
    		if( ! aar.getGoon() || aar.getTimeRemained() <= 0 ){
    			//println("			WARNING: sense timeout");
    			addRule("tout(senseevent,"+getName()+")");
    		}
    		if( planUtils.repeatPlan(nPlanIter,0).getGoon() ) continue;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=standby WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean turnLeft90() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "turnLeft90";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "turnLeft90";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN TURN LEFT 90------------------\"";
    		println( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?rotationLeftValues(S,T)" )) != null ){
    		//left
    		//if( ! execRobotMove("turnLeft90","left",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "") ) break;
    		    aar = execRobotMove("turnLeft90","left",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "turnLeft90";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?saveMove(lft)" )) != null ){
    		//we should restore nPlanIter and curPlanInExec of the 'interrupted' plan ???
    		returnValue = continueWork;
    		}
    		//we should restore nPlanIter and curPlanInExec of the 'interrupted' plan ???
    		returnValue = continueWork;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=turnLeft90 WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean turnLeft180() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "turnLeft180";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "turnLeft180";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN TURN LEFT 180------------------\"";
    		println( temporaryStr );  
    		if( ! planUtils.switchToPlan("turnLeft90").getGoon() ) break;
    		//delay
    		aar = delayReactive(500,"" , "");
    		if( aar.getInterrupted() ) curPlanInExec   = "turnLeft180";
    		if( ! aar.getGoon() ) break;
    		if( ! planUtils.switchToPlan("turnLeft90").getGoon() ) break;
    		//we should restore nPlanIter and curPlanInExec of the 'interrupted' plan ???
    		returnValue = continueWork;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=turnLeft180 WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
