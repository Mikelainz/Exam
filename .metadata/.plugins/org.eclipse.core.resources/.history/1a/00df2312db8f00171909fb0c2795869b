    			if( ! aar.getGoon() ) break;
    		} 			
    		//QActorUtils.solveGoal(parg,pengine );
    		}
    		temporaryStr = "goingForwardHome";
    		addRule( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?isNextMove(fwd,S,T)" )) != null ){
    		//forward
    		//if( ! execRobotMove("goForwardHome","forward",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "local_alarm,local_takeControl,sonarDetect" , "handleAlarmReleased,handleTakeControl,handleObstacleGoHome") ) break;
    		    aar = execRobotMove("goForwardHome","forward",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "local_alarm,local_takeControl,sonarDetect" , "handleAlarmReleased,handleTakeControl,handleObstacleGoHome");
