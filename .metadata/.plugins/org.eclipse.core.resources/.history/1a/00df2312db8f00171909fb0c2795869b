    public boolean controlReleasedStandby() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "controlReleasedStandby";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
