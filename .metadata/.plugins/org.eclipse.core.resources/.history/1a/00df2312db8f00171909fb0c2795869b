    while(true){
    	curPlanInExec =  "homeReached";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN HOME REACHED------------------\"";
    		println( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " ??searchLeft" )) != null ){
    		temporaryStr = "\"[ROVER] searchLeft Rule Deleted!\"";
    		temporaryStr = QActorUtils.substituteVars(guardVars,temporaryStr);
    		println( temporaryStr );  
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?rotationRightValues(S,T)" )) != null ){
    		//right
    		//if( ! execRobotMove("homeReached","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "") ) break;
    		    aar = execRobotMove("homeReached","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "homeReached";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?rotationRightValues(S,T)" )) != null ){
    		//right
    		//if( ! execRobotMove("homeReached","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "") ) break;
    		    aar = execRobotMove("homeReached","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "homeReached";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " ??goingHome" )) != null ){
    		temporaryStr = "\"[ROVER] goingHome Rule Deleted!\"";
    		temporaryStr = QActorUtils.substituteVars(guardVars,temporaryStr);
    		println( temporaryStr );  
    		}
    		if( ! planUtils.switchToPlan("standby").getGoon() ) break;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=homeReached WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
