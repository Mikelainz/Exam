    		//forward
    		//if( ! execRobotMove("goForward","forward",Integer.parseInt(guardVars.get("FS")),0,60000, "local_sonar2Support,sonarDetect,local_alarm,local_takeControl" , "handleSonar2Support,handleObstacle,handleAlarm,handleTakeControl") ) break;
    		    aar = execRobotMove("goForward","forward",Integer.parseInt(guardVars.get("FS")),0,60000, "local_sonar2Support,sonarDetect,local_alarm,local_takeControl" , "handleSonar2Support,handleObstacle,handleAlarm,handleTakeControl");
