    public boolean handleGoHome() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "handleGoHome";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "handleGoHome";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN HANDLE GO HOME------------------\"";
    		println( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?clearPath(L)" )) != null ){
    		temporaryStr = "myPathList(L)";
    		temporaryStr = QActorUtils.substituteVars(guardVars,temporaryStr);
    		println( temporaryStr );  
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?goingHome" )) != null ){
    		if( ! planUtils.switchToPlan("goHome").getGoon() ) break;
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?rotationRightValues(S,T)" )) != null ){
    		//right
    		//if( ! execRobotMove("handleGoHome","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "") ) break;
    		    aar = execRobotMove("handleGoHome","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "handleGoHome";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		//delay
    		aar = delayReactive(500,"" , "");
    		if( aar.getInterrupted() ) curPlanInExec   = "handleGoHome";
    		if( ! aar.getGoon() ) break;
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?rotationRightValues(S,T)" )) != null ){
    		//right
    		//if( ! execRobotMove("handleGoHome","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "") ) break;
    		    aar = execRobotMove("handleGoHome","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "handleGoHome";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		temporaryStr = "goingHome";
    		addRule( temporaryStr );  
    		if( ! planUtils.switchToPlan("goHome").getGoon() ) break;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=handleGoHome WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean handleObstacle() throws Exception{	//public to allow reflection
