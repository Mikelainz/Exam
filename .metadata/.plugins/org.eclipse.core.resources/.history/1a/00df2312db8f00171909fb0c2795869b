    		    	curPlanInExec   = "handleObstacle";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		temporaryStr = QActorUtils.unifyMsgContent(pengine,"checkObstacleType","checkObstacleType", guardVars ).toString();
    		sendMsg("checkObstacleType","obstaclesupport", QActorContext.dispatch, temporaryStr ); 
    		if( ! planUtils.switchToPlan("typeOfObstacle").getGoon() ) break;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=handleObstacle WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean handleObstacleGoHome() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "handleObstacleGoHome";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "handleObstacleGoHome";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN HANDLE OBSTACLE GO HOME------------------\"";
    		println( temporaryStr );  
    		//stop
    		//if( ! execRobotMove("handleObstacleGoHome","stop",0,0,0, "" , "") ) break;
    		    aar = execRobotMove("handleObstacleGoHome","stop",0,0,0, "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "handleObstacleGoHome";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		if( (guardVars = QActorUtils.evalTheGuard(this, " ??goingForwardHome" )) != null ){
    		if( ! planUtils.switchToPlan("saveRemainingTime").getGoon() ) break;
    		}
    		temporaryStr = QActorUtils.unifyMsgContent(pengine,"checkObstacleType","checkObstacleType", guardVars ).toString();
    		sendMsg("checkObstacleType","obstaclesupport", QActorContext.dispatch, temporaryStr ); 
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?senseTimeFixedObstacle(T)" )) != null ){
    		//senseEvent
    		aar = planUtils.senseEvents( Integer.parseInt(QActorUtils.substituteVars(guardVars,"T").replace("'","")),"noMoreObstacle","goHome",
    		"local_takeControl" , "handleTakeControl",ActionExecMode.synch );
    		if( ! aar.getGoon() || aar.getTimeRemained() <= 0 ){
    			//println("			WARNING: sense timeout");
    			addRule("tout(senseevent,"+getName()+")");
    		}
    		}
    		if( ! planUtils.switchToPlan("goHome").getGoon() ) break;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=handleObstacleGoHome WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
