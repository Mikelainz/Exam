    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " ??goingForwardHome" )) != null ){
    		temporaryStr = "\"[ROVER] going forward home deleted\"";
    		temporaryStr = QActorUtils.substituteVars(guardVars,temporaryStr);
    		println( temporaryStr );  
    		}
    		//we should restore nPlanIter and curPlanInExec of the 'interrupted' plan ???
    		returnValue = continueWork;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=goForwardHome WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean goHome() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "goHome";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "goHome";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN GO HOME------------------\"";
    		println( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?isNextMove(fwd,S,T)" )) != null ){
    		if( ! planUtils.switchToPlan("goForwardHome").getGoon() ) break;
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?isNextMove(lft,S,T)" )) != null ){
    		//right
    		//if( ! execRobotMove("goHome","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "") ) break;
    		    aar = execRobotMove("goHome","right",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "goHome";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?isNextMove(rgt,S,T)" )) != null ){
    		//left
    		//if( ! execRobotMove("goHome","left",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "") ) break;
    		    aar = execRobotMove("goHome","left",Integer.parseInt(guardVars.get("S")),0,Integer.parseInt(guardVars.get("T")), "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "goHome";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?deleteLastMove(MOVE)" )) != null ){
    		temporaryStr = "lastMoveDeleted(MOVE)";
    		temporaryStr = QActorUtils.substituteVars(guardVars,temporaryStr);
    		println( temporaryStr );  
    		}
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?pathEmpty" )) != null ){
    		if( ! planUtils.switchToPlan("homeReached").getGoon() ) break;
    		}
    		else{ if( planUtils.repeatPlan(nPlanIter,0).getGoon() ) continue;
    		}
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=goHome WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
    public boolean handleAlarm() throws Exception{	//public to allow reflection
    try{
    	int nPlanIter = 0;
    	//curPlanInExec =  "handleAlarm";
    	boolean returnValue = suspendWork;		//MARCHH2017
    while(true){
    	curPlanInExec =  "handleAlarm";	//within while since it can be lost by switchlan
    	nPlanIter++;
    		temporaryStr = "\"------------------PLAN HANDLE ALLARM------------------\"";
    		println( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " ??goingForward" )) != null ){
    		if( ! planUtils.switchToPlan("saveForwardPath").getGoon() ) break;
    		}
    		//stop
    		//if( ! execRobotMove("handleAlarm","stop",0,0,0, "" , "") ) break;
    		    aar = execRobotMove("handleAlarm","stop",0,0,0, "" , "");
    		    if( aar.getInterrupted() ){
    		    	curPlanInExec   = "handleAlarm";
    		    	if( ! aar.getGoon() ) break;
    		    } 			
    		if( (guardVars = QActorUtils.evalTheGuard(this, " ??noAlarmFDR" )) != null ){
    		parg = "actorOp(decAttemptRight)";
    		parg = QActorUtils.substituteVars(guardVars,parg);
    		aar = solveGoalReactive(parg,3600000,"","");
    		//println(getName() + " plan " + curPlanInExec  +  " interrupted=" + aar.getInterrupted() + " action goon="+aar.getGoon());
    		if( aar.getInterrupted() ){
    			curPlanInExec   = "handleAlarm";
    			if( aar.getTimeRemained() <= 0 ) addRule("tout(actorOp,"+getName()+")");
    			if( ! aar.getGoon() ) break;
    		} 			
    		//QActorUtils.solveGoal(parg,pengine );
    		}
    		//senseEvent
    		aar = planUtils.senseEvents( 600000,"local_start,local_takeControl","continue,handleTakeControl",
    		"" , "",ActionExecMode.synch );
    		if( ! aar.getGoon() || aar.getTimeRemained() <= 0 ){
    			//println("			WARNING: sense timeout");
    			addRule("tout(senseevent,"+getName()+")");
    		}
    		//onEvent
    		if( currentEvent.getEventId().equals("local_start") ){
    		 		//println("WARNING: variable substitution not yet implmented " ); 
    		 		//we should restore nPlanIter and curPlanInExec of the 'interrupted' plan ???
    		 		returnValue = continueWork;
    		 }
    		//we should restore nPlanIter and curPlanInExec of the 'interrupted' plan ???
    		returnValue = continueWork;
    break;
    }//while
    return returnValue;
    }catch(Exception e){
       //println( getName() + " plan=handleAlarm WARNING:" + e.getMessage() );
       QActorContext.terminateQActorSystem(this); 
       return false;  
    }
    }
