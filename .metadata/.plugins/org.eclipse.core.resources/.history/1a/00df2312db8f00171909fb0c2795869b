    		if( ! planUtils.switchToPlan("saveForwardPath").getGoon() ) break;
    		}
    		//stop
    		//if( ! execRobotMove("handleObstacle","stop",0,0,0, "" , "") ) break;
    		    aar = execRobotMove("handleObstacle","stop",0,0,0, "" , "");
