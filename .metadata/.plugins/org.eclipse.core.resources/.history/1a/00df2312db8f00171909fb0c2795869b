    		temporaryStr = "\"------------------PLAN GO FORWARD HOME------------------\"";
    		println( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?isNextMove(fwd,S,T)" )) != null ){
    		parg = "actorOp(saveStartGoHome(T))";
    		parg = QActorUtils.substituteVars(guardVars,parg);
    		aar = solveGoalReactive(parg,3600000,"","");
