    		println( temporaryStr );  
    		if( (guardVars = QActorUtils.evalTheGuard(this, " !?goLeft" )) != null ){
    		if( ! planUtils.switchToPlan("findTheDoorLeft").getGoon() ) break;
    		}
    		if( ! planUtils.switchToPlan("findTheDoorRight").getGoon() ) break;
    break;
    }//while
